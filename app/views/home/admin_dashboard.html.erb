<h1>Hello admin </h1>
<div class="table-responsive">
      <table class="table">
        <thead>
          <tr>
            <th>Artist </th>
            <th> show </th>
            <th> Edit </th>
            <th>Delete</th>
          </tr>
        </thead>
        <tbody>
          <%@artist.each do |art| %>
            <tr>
              <td><%= art.first_name %></td>
              <td><%= link_to "Show",artist_path(art) %></td>
              <td><%= link_to "Edit", edit_artist_path(art) %></td>
              <td><%= link_to "Delete",artist_path(art), method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <%end%>
        </tbody>
      </table>
    </div>
